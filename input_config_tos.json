{
  "metrics": {
    "plugins": [
      {
        "name": "linux"
      },
      {
        "name": "topstats",
        "interval": 10,
        "config": {
          "process": "*",
          "resource": "CPU",
          "maximum_grep": 5
        }
      },
      {
        "name": "elasticsearchagent",
        "config": {
          "port": 9200,
          "es_protocol": "http",
          "es_username": "elastic",
          "es_password": "elastic",
          "documentsTypes": [
            "clusterStats",
            "nodeStats",
            "indexStats"
          ]
        }
      }
    ],
    "enabled": true,
    "targets": [
      "metric-apm"
    ],
    "tags": {
      "projectName": "@p",
      "Name": "@h",
      "appName": "@a"
    }
  },
  "logging": {
    "plugins": [
      {
        "name": "linux-syslog",
        "config": {
          "filters": {
            "level": [
              "error","warning","info"
            ]
          }
        }
      }
    ],
    "custom-plugins": [
      {
        "name": "alertaccess",
        "parser": "/opt/sfapm/td-agent-bit/templates/alertaccess_parser.conf",
        "plugin": "/opt/sfapm/td-agent-bit/templates/alertaccess_config.conf",
        "collection_type": "logger",
        "field_discovery": "Off",
        "add_offset": "@t",
        "number_fields": "size,code",
        "string_fields": "",
        "time_fields": "",
        "field_extracters":{
          "component1": "(?<component1>\\[.*\\])",
          "component2": "(?<component2>[a-zA-Z0-9]+):\\d+$"
        }
      },
      {
        "name": "clientmux",
        "parser": "/opt/sfapm/td-agent-bit/templates/clientmux_parser.conf",
        "plugin": "/opt/sfapm/td-agent-bit/templates/clientmux_config.conf",
        "collection_type": "logger",
        "field_discovery": "Off",
        "add_offset": "@t",
        "number_fields": "size,code",
        "string_fields": "",
        "time_fields": ""
      },
      {
        "name": "restarts",
        "parser": "/opt/sfapm/td-agent-bit/templates/restarts_parser.conf",
        "plugin": "/opt/sfapm/td-agent-bit/templates/restarts_config.conf",
        "log_path": "/app/tosbqa/log/restarts.log",
        "collection_type": "logger",
        "field_discovery": "Off",
        "add_offset": "@t",
        "number_fields": "",
        "string_fields": "",
        "time_fields": ""
      },
      {
        "name": "server_admin_openapi",
        "parser": "/opt/sfapm/td-agent-bit/templates/server_admin_openapi_parser.conf",
        "plugin": "/opt/sfapm/td-agent-bit/templates/server_admin_openapi_config.conf",
        "collection_type": "logger",
        "field_discovery": "Off",
        "number_fields": "",
        "string_fields": "",
        "time_fields": ""
