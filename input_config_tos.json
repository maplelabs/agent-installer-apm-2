    "enabled": true,
    "targets": [
      "log-apm",
      "metric-apm"
    ],
    "tags": {
      "projectName": "@p",
      "Name": "@h",
      "appName": "@a"
    }
  },
  "name": "@h",
  "port": 8585,
  "application": "redis-load-test",
  "targets": [
    {
      "config": {
        "index": "@p-metric",
        "rollover_max_docs": "None",
        "protocol": "http",
        "indexType": "roll_over",
        "replicas": 1,
        "shards": 3,
        "rollover_max_age": "1d",
        "host": "@e",
        "store_type": "metric",
        "rollover": "True",
        "port": 9200,
        "username": "elastic",
        "password": "elastic"
      },
      "type": "elasticsearch",
      "name": "metric-apm"
    },
    {
      "config": {
        "index": "@p-logger",
        "rollover_max_docs": "None",
        "protocol": "http",
        "indexType": "roll_over",
        "replicas": 1,
        "shards": 3,
        "rollover_max_age": "1d",
        "host": "@e",
        "store_type": "logger",
        "rollover": "True",
        "port": 9200,
        "username": "elastic",
        "password": "elastic"
      },
      "type": "elasticsearch",
      "name": "log-apm"
    }
  ]
}
