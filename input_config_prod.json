        "collection_type": "logger",
        "field_discovery": "On",
        "add_offset": "On",
        "number_fields": "pendingRequestCount,totalWaitForConnectionTime,AdamClass,member,mdCount,bebEventProcessTime,QuoteResponse,numofaccounts,elapsedTime,delayTime,symbolcountrequested,symboncountreturned,notLocatedOrderId,quote_resp_ms",
