{
  "metrics": {
    "plugins": [
      {
        "name": "linux"
      },
      {
        "name": "psstats",
        "config": {
          "num_processes": "*",
          "sort_by": "CPU"
        }
      },
      {
        "name": "elasticsearchagent",
        "config": {
          "port": 9200,
          "es_protocol": "http",
          "es_username": "elastic",
          "es_password": "elastic",
          "documentsTypes": [
            "clusterStats",
            "nodeStats",
            "indexStats"
          ]
        }
      }
    ],
    "enabled": true,
    "targets": [
      "metric-apm"
    ],
    "tags": {
      "projectName": "@p",
      "Name": "@h",
      "appName": "@a"
    }
  },
  "logging": {
    "plugins": [
      {
        "name": "linux-syslog",
        "config": {
          "filters": {
            "level": [
              "error","warning","info"
            ]
          }
        }
      }
    ],
    "custom-plugins": [
      {
        "name": "ngoms_grid",
        "parser": "/opt/sfapm/td-agent-bit/templates/ngoms_grid_parser.conf",
        "plugin": "/opt/sfapm/td-agent-bit/templates/ngoms_grid_config.conf",
        "collection_type": "logger",
        "field_discovery": "On",
        "add_offset": "On",
        "number_fields": "pendingRequestCount,totalWaitForConnectionTime,member,mdCount,bebEventProcessTime,QuoteResponse,numofaccounts,elapsedTime,delayTime,symbolcountrequested,symbolcountreturned,notLocatedOrderId,quote_resp_ms",
        "string_fields": "poolName",
        "time_fields": "Timestamp,startTime,endTime"
      }
    ],
    "enabled": true,
    "targets": [
      "log-apm",
      "metric-apm"
    ],
    "tags": {
      "projectName": "@p",
      "Name": "@h",
      "appName": "@a"
    }
  },
  "name": "@h",
  "port": 8585,
  "application": "application-test",
  "targets": [
    {
      "config": {
        "index": "@p-metric",
        "rollover_max_docs": "None",
        "protocol": "http",
        "indexType": "roll_over",
        "replicas": 1,
        "shards": 3,
        "rollover_max_age": "1d",
        "host": "@e",
        "store_type": "metric",
        "rollover": "True",
        "port": 9200,
        "username": "elastic",
        "password": "elastic"
      },
      "type": "elasticsearch",
      "name": "metric-apm"
    },
    {
      "config": {
        "index": "@p-logger",
        "rollover_max_docs": "None",
        "protocol": "http",
        "indexType": "roll_over",
        "replicas": 1,
        "shards": 3,
        "rollover_max_age": "1d",
        "host": "@e",
        "store_type": "logger",
        "rollover": "True",
        "port": 9200,
        "username": "elastic",
        "password": "elastic"
      },
      "type": "elasticsearch",
      "name": "log-apm"
    }
  ]
}
